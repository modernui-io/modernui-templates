{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-native",
    "noUncheckedIndexedAccess": true,
    "paths": {
      "@/assets/*": ["./assets/*"],
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
