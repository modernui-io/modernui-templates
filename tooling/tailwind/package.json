{
  "name": "@modernui/tailwind-config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./native": "./native.ts",
    "./web": "./web.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "============= FORMAT =================": "",
    "format": "biome format --write .",
    "============= LINT =================": "",
    "lint": "biome check . --max-diagnostics=50",
    "lint:fix": "biome check --write . --max-diagnostics=50",
    "lint:fix:errors": "biome check --write . --diagnostic-level=error --max-diagnostics=50",
    "============= TYPE-CHECK =================": "",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@fluid-tailwind/tailwind-merge": "^0.0.3",
    "@iconify-json/logos": "^1.2.3",
    "@iconify-json/lucide": "^1.2.18",
    "@iconify-json/ph": "^1.2.1",
    "@iconify-json/ri": "^1.2.3",
    "@iconify-json/svg-spinners": "^1.2.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "fluid-tailwind": "^1.0.4",
    "postcss": "^8.4.49",
    "postcss-hover-media-feature": "^1.0.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@modernui/tsconfig": "workspace:*",
    "typescript": "^5.7.2"
  }
}
