{
  "name": "@modernui/template-expo",
  "main": "index.ts",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "expo start -c --ios",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "============= CLEAN =================": "",
    "clean": "git clean -xdf .expo .turbo node_modules",
    "============= FORMAT =================": "",
    "format": "biome format --write .",
    "============= LINT =================": "",
    "lint": "biome check .",
    "lint:fix": "biome check --write . --max-diagnostics=50",
    "============= TYPE-CHECK =================": "",
    "type-check": "tsc --noEmit",
    "============= Expo Installs Setting =================": "",
    "chk:config": "expo config --full",
    "chk:pkgs": "expo install --check",
    "fix:pkgs": "expo install --fix",
    "============= Styling build =================": "",
    "postinstall": "npx --yes tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.0",
    "@react-navigation/native": "^7.0.13",
    "@rn-primitives/avatar": "~1.1.0",
    "@rn-primitives/portal": "~1.1.0",
    "@rn-primitives/progress": "~1.1.0",
    "@rn-primitives/slot": "~1.1.0",
    "@rn-primitives/tooltip": "~1.1.0",
    "@rn-primitives/types": "~1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^52.0.17",
    "expo-linking": "~7.0.3",
    "expo-navigation-bar": "~4.0.5",
    "expo-router": "~4.0.11",
    "expo-splash-screen": "~0.29.16",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.5",
    "lucide-react-native": "^0.468.0",
    "nativewind": "^4.1.23",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "0.76.4",
    "react-native-reanimated": "~3.16.3",
    "react-native-safe-area-context": "5.0.0",
    "react-native-screens": "^4.3.0",
    "react-native-svg": "15.10.1",
    "react-native-web": "~0.19.13",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "^19.0.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2"
  }
}
