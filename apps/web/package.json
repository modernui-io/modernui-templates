    "@biomejs/biome": "1.9.4",
    "@modernui/tsconfig": "workspace:*",
    "@types/node": "^22",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16"
